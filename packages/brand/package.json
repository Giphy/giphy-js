{
    "scripts": {
        "clean": "rm -rf ./dist ./esm",
        "dev": "parcel public/test.html",
        "build": "tsc && npm run build:esm",
        "build:esm": "tsc -p ./tsconfig-esm.json",
        "prepublish": "npm run clean && npm run build"
    },
    "devDependencies": {
        "parcel-bundler": "latest",
        "parcel-plugin-bundle-visualiser": "1.2.0",
        "parcel-plugin-url-loader": "^1.3.1",
        "preact": "next",
        "typescript": "^5.0.4"
    },
    "name": "@giphy/js-brand",
    "version": "2.2.3-alpha.0",
    "main": "./dist/index.js",
    "description": "colors, fonts, and other shared css",
    "homepage": "https://github.com/Giphy/giphy-js/tree/master/packages/brand",
    "types": "./dist/index.d.ts",
    "module": "esm/index.js",
    "sideEffects": [
        "./src/typography.ts"
    ],
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist/",
        "esm/",
        "src/**/*"
    ],
    "author": "giannif",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "source": true,
    "dependencies": {
        "emotion": "10.0.27"
    }
}
