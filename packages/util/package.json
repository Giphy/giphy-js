{
    "scripts": {
        "lint": "run -T eslint . --ext .ts,.tsx",
        "clean": "rm -rf ./dist ./esm",
        "dev": "tsc --watch",
        "build": "tsc && npm run build:esm",
        "build:esm": "tsc -p ./tsconfig-esm.json",
        "prepublish": "npm run clean && npm run build",
        "test": "run -T jest --config ./jestconfig.js",
        "test:watch": "run -T jest --config ./jestconfig.js --watchAll"
    },
    "name": "@giphy/js-util",
    "version": "4.4.2",
    "main": "dist/index.js",
    "description": "Shared giphy js utils",
    "homepage": "https://github.com/Giphy/giphy-js/tree/master/packages/util",
    "types": "dist/index.d.ts",
    "module": "esm/index.js",
    "sideEffects": false,
    "files": [
        "dist/",
        "esm/",
        "src/**/*"
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@giphy/js-types": "*",
        "dompurify": "^2.2.2",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/dompurify": "^2.0.4",
        "@types/uuid": "^8.3.0",
        "typescript": "^5.0.4"
    }
}
