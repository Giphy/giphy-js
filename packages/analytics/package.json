{
    "scripts": {
        "clean": "rm -rf ./dist ./esm",
        "dev": "tsc --watch",
        "build": "tsc && npm run build:esm",
        "build:esm": "tsc -p ./tsconfig-esm.json",
        "prepublish": "npm run clean && npm run build",
        "test": "jest --config ./jestconfig.js",
        "test:watch": "jest --config ./jestconfig.js --watchAll"
    },
    "name": "@giphy/js-analytics",
    "homepage": "https://github.com/Giphy/giphy-js/tree/master/packages/analytics",
    "version": "4.3.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "esm/index.js",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist/",
        "esm/",
        "src/**/*"
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@giphy/js-types": "*",
        "@giphy/js-util": "*",
        "append-query": "^2.1.0",
        "throttle-debounce": "^3.0.1"
    },
    "devDependencies": {
        "@types/append-query": "^2.0.0",
        "@types/throttle-debounce": "2.1.0",
        "jest-fetch-mock": "^3.0.3",
        "typescript": "^5.0.4"
    }
}
